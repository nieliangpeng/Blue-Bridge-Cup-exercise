#include<iostream>
#include<string.h>
#include<time.h> 
using namespace std;
bool text(int year){
	if((year%4==0&&year%100!=0)||(year%400==0)){
		return true;
		
	}
	else 
	return false;
}
int main(void)
{
 int year=2015;
 int sum=1000-52;
 int yearsum,i,month=0;
 int months[13]={0,31,28,31,30,31,30,31,31,30,31,30,31};
 while(1){
 	if(text(year)){
 		yearsum=366;
 	}
 	else{
 		yearsum=365;
 	}
 	if(sum>yearsum){
 		sum=sum-yearsum;
 		year++;
 	}
 	else{
 		
 		break;
 	}
 }
 if(text(year)) months[2]=29;
 for(i=1;i<=12;i++){
 	if(sum>months[i]) {
 		sum=sum-months[i];
 		month+=1;
 	}
 	else{
 		month+=1;
 		break;
 	}
 }
 cout<<year<<" "<<month<<" "<<sum;
 return 0;
}
